{
    "name": "mmorinaga/wrench",
    "description": "Maintenance mode plugin for CakePHP 3.X",
    "type": "cakephp-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "Yves Piquel",
            "homepage": "https://github.com/HavokInspiration/wrench"
        }
    ],
    "support": {
        "issues": "https://github.com/HavokInspiration/wrench/issues",
        "source": "https://github.com/HavokInspiration/wrench"
    },
    "require": {
        "php": ">=8.1",
        "cakephp/cakephp": "^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "cakephp/bake": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Wrench\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Wrench\\Test\\": "tests/",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests/",
            "App\\": "tests/test_app/App/",
            "TestPlugin\\": "tests/test_app/plugins/TestPlugin/src/"
        }
    }
}
